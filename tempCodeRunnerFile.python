import pyxel

class Game:
    def __init__(self):
        pyxel.init(160, 120)
        pyxel.load("main.pyxres")
        pyxel.run(self.update, self.draw)

    def update(self):
        if pyxel.btnp(pyxel.KEY_Q):
            pyxel.quit()

    def draw(self):
        pyxel.cls(0)
